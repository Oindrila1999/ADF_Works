{
	"name": "flowlet_remove_dup",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "aggregate1"
				},
				{
					"name": "select1"
				},
				{
					"name": "input1"
				}
			],
			"scriptLines": [
				"input(output(",
				"          id as integer,",
				"          name as string,",
				"          gender as string,",
				"          deptid as integer,",
				"          deptname as string",
				"     ),",
				"     order: 0,",
				"     allowSchemaDrift: true) ~> input1",
				"input1 aggregate(groupBy(id,",
				"          name,",
				"          gender,",
				"          deptid,",
				"          deptname),",
				"     count = count()) ~> aggregate1",
				"aggregate1 select(mapColumn(",
				"          id,",
				"          name,",
				"          gender,",
				"          deptid,",
				"          deptname",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}